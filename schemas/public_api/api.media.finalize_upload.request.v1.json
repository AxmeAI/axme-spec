{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://axme.dev/schemas/public_api/api.media.finalize_upload.request.v1.json",
  "title": "ApiMediaFinalizeUploadRequestV1",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "upload_id",
    "size_bytes"
  ],
  "properties": {
    "upload_id": {
      "type": "string",
      "format": "uuid"
    },
    "size_bytes": {
      "type": "integer",
      "minimum": 1,
      "maximum": 2147483648
    },
    "sha256": {
      "type": "string",
      "pattern": "^[a-f0-9]{64}$"
    }
  }
}
