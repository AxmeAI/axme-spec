{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://axme.dev/schemas/public_api/api.intents.create.response.v1.json",
  "title": "ApiIntentsCreateResponseV1",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "ok",
    "intent_id",
    "status",
    "created_at"
  ],
  "properties": {
    "ok": {
      "type": "boolean",
      "const": true
    },
    "intent_id": {
      "type": "string",
      "format": "uuid"
    },
    "status": {
      "type": "string",
      "enum": [
        "accepted",
        "running",
        "blocked",
        "done",
        "failed"
      ]
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    }
  }
}
